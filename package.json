    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development node ./config/webpack/console_server.js",
    "start-over": "yarn clean-webpack && yarn start",
    "stylelint": "./node_modules/.bin/stylelint --color --max-warnings 0 --config \"stylelint.config.js\" \"src/**/*.scss\" \"src/**/*.css\" \"src/**/*.sass\" --cache true --cache-location ./.lintcache/stylelintcache/",
